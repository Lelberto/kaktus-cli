{
  "name": "kaktus-cli",
  "description": "CLI for Kaktus",
  "version": "1.0.0",
  "author": "Lelberto @Lelberto",
  "bin": {
    "kaktus": "./bin/run"
  },
  "bugs": "https://github.com/Lelberto/kaktus-cli/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.7",
    "@types/lodash": "^4.14.178",
    "@types/node": "^10.17.60",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^3.1.2",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^10.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Lelberto/kaktus-cli",
  "keywords": [
    "kaktus",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "kaktus",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Lelberto/kaktus-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.8",
    "@oclif/config": "^1.18.2",
    "@oclif/plugin-help": "^3.2.12",
    "axios": "^0.24.0",
    "extract-zip": "^2.0.1",
    "lodash": "^4.17.21",
    "tslib": "^1.14.1"
  }
}
